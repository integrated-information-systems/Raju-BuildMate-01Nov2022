                    if (isNaN(calculated)) {
                        self.SQ.discAmount(0);
                    }
                    else {                        
                        calculated = roundToTwo(calculated);
                        self.SQ.discAmount(calculated);
                    }
                }
                else {

                    var calculated = 0;
                    if (!isNaN(self.SQ.netTotal()) && !isNaN(self.SQ.discAmount())) {

                        calculated = (self.SQ.discAmount() * 100) / self.SQ.netTotal();
                    }
                    if (isNaN(calculated)) {
                        self.SQ.discPercent(0);
                    }
                    else {
                        calculated = roundToTwo(calculated);
                        self.SQ.discPercent(calculated);
                    }
                }

