            self.CalcDiscounts = function () {

                if (self.SQ.discByPercent() === "true") {

                    var calculated = 0;
                    if (!isNaN(self.SQ.netTotal()) && !isNaN(self.SQ.discPercent())) {
                        calculated = self.SQ.netTotal() * (self.SQ.discPercent() / 100);
