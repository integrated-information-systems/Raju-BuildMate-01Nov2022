                        { className: "text-right", "targets": [2, 3] }
                    ]
                });
            var NoteListTable = $('#NoteListTable').DataTable(
                {
                                
                });
                
          
            var PriceBreaktable = $('#PriceBreak').DataTable(
                {
                    "columnDefs": [
                        { className: "text-right", "targets": [1, 2] }
                    ],
                    "paging": false,
                    "ordering": false,
                    "info": false
                });
 
