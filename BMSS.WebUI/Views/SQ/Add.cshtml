            self.CalcTotals = function () {
                var GrandTotal = 0;
                if ( !isNaN(self.SQ.netTotal()) && !isNaN(self.SQ.discAmount()) && !isNaN(self.SQ.gstTotal())) {
                    GrandTotal = parseFloat(self.SQ.netTotal()) - parseFloat(self.SQ.discAmount()) + parseFloat(self.SQ.gstTotal());

                }
                if (isNaN(GrandTotal)) {
                    self.SQ.grandTotal(0);
                }
                else {                   
                    GrandTotal = roundToTwo(GrandTotal);
                    self.SQ.grandTotal(GrandTotal);
                }
                
