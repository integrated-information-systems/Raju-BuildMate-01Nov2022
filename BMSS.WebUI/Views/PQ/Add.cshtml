}
@section Content_Header {
    <section class="content-header">
        <h1>
            @ViewBag.Title
            <small>Purchase Quotation</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i>Transactions</a></li>
            <li class="active"><a href='@Url.Action("Index", "PQ")'>Purchase Quotations</a></li>
        </ol>
    </section>
}
